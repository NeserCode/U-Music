{
  "build": {
    "beforeDevCommand": "yarn dev",
    "beforeBuildCommand": "yarn build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:8888"
  },
  "bundle": {
    "active": true,
    "targets": "nsis",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "U Music",
  "mainBinaryName": "U Music",
  "version": "0.0.0",
  "identifier": "com.nesercode.umusic-beta",
  "plugins": {},
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "title": "U Music",
        "width": 800,
        "height": 600,
        "minWidth": 800,
        "minHeight": 600,
        "decorations": false,
        "shadow": false,
        "transparent": true,
        "visible": false,
        "useHttpsScheme": true
      }
    ]
  }
}